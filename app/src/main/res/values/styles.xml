<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--<item name="android:listDivider">@drawable/list_diliver</item>-->
    </style>

    <style name="small_textStyle" parent="AppTheme">
        <item name="textSizeTextView">@dimen/textSize_11sp</item>
        <item name="textSizeButtom">@dimen/textSize_20sp</item>
    </style>
    <style name="default_textStyle" parent="AppTheme">
    </style>
    <style name="big_textStyle" parent="AppTheme">
        <item name="textSizeTextView">@dimen/textSize_30sp</item>
        <item name="textSizeButtom">@dimen/textSize_60sp</item>
    </style>
    <style name="another_textStyle" parent="AppTheme">
        <item name="textSizeTextView">100sp</item>
    </style>






</resources>
